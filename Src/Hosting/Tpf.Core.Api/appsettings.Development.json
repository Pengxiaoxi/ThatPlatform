{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Grpc": "Information"
    }
  },
  "AllowedHosts": "*",

  // Tpf.Config
  "Project": {
    "Middlewares": {
      "Swagger": {
        "IsEnable": "true"
      }
    },
    "ORM": {
      "MainORM": "EFCore",
      "SlaveORM": "Dapper",
      "DB": {
        "ConnectionString": ""
      }
    },
    "Cache": {
      "IsEnable": "false",
      "CacheType": "Redis", // Redis|MemoryCache
      "RedisConfig": {

      },
      "MemoryCacheConfig": {

      }
    },
    "Job": {
      "IsEnable": "false",
      "JobType": "Quartz", // Quartz|Hangfire
      "QuartzConfig": {

        "JobConfig": {
          "MongoDBBackupCron": "0/5 * * * * ? " // Cron表达式：5s 执行一次  https://cron.qqe2.com/ 
        }
      }
    }
  },
  // Others
  "Others": {

  }

